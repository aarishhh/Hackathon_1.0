# Security Configuration
JWT_SECRET=your-super-secure-jwt-secret-key-minimum-256-bits
JWT_REFRESH_SECRET=your-super-secure-refresh-secret-key
JWT_EXPIRE=15m
JWT_REFRESH_EXPIRE=7d

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/security-first-gov-app
DB_ENCRYPTION_KEY=your-database-encryption-key-32-chars

# Server Configuration
NODE_ENV=development
PORT=3001
CORS_ORIGIN=http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Encryption Keys
AES_ENCRYPTION_KEY=your-aes-256-encryption-key-here-32-chars
IV_LENGTH=16

# MFA Configuration
MFA_SERVICE_NAME=SecureGovApp
MFA_ISSUER=Government of India

# Logging
LOG_LEVEL=info
LOG_FILE=logs/security-audit.log

# Session Configuration
SESSION_SECRET=your-session-secret-key-here
COOKIE_SECURE=false
COOKIE_HTTP_ONLY=true
COOKIE_SAME_SITE=strict
